{
  "name": "ducklingscript-language-support",
  "displayName": "DucklingScript Language for VSCode",
  "description": "A basic implementaton of DucklingScript syntax support to VSCode.",
  "publisher": "DragonOfShuu",
  "version": "0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/deathhancox/ducklingscript-language-support.git"
  },
  "contributes": {
    "languages": [{
      "id": "dkls",
      "aliases": ["DucklingScript", "dkls"],
      "extensions": [".dkls"],
      "configuration": "./language-configuration.json",
      "icon": {
        "dark": "./static/dark-file-icon.png",
        "light": "./static/light-file-icon.png"
      }
    }],
    "grammars": [{
      "language": "dkls",
      "scopeName": "source.ducklingscript",
      "path": "./syntaxes/dkls.tmLanguage.json"
    }]
  },
  "icon": "./static/extension-icon.png"
}
