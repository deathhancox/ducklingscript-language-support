{
  "name": "ducklingscript-language-support",
  "displayName": "DucklingScript Language Support",
  "description": "A basic implamentaton of DucklingScript syntax support to VSCode.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/deathhancox/ducklingscript-language-support.git"
  },
  "contributes": {
    "languages": [{
      "id": "dkls",
      "aliases": ["DucklingScript", "dkls"],
      "extensions": [".dkls"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "dkls",
      "scopeName": "source.ducklingscript",
      "path": "./syntaxes/dkls.tmLanguage.json"
    }]
  }
}
